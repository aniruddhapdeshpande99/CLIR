02 \u0915\u0947 \u0932\u093f\u090f \u091a\u092f\u0928\u093f\u0924 \u0915\u093f\u090f \u0917\u090f 112 \u0932\u094b\u0917\u094b\u0902 \u092e\u0947\u0902 \u0938\u0947 56 \u0932\u094b\u0917\u094b\u0902 \u0915\u094b \u0938\u092e\u094d\u092e\u093e\u0928\u093f\u0924 \u0915\u093f\u092f\u093e\u0964 \u0907\u0928 \u0932\u094b\u0917\u094b\u0902 \u092e\u0947\u0902 \u092e\u0928\u094b\u0930\u0902\u091c\u0928 \u0915\u0940 \u0926\u0941\u0928\u093f\u092f\u093e \u0938\u0947 \u091c\u0941\u0921\u093c\u0947 \u092e\u094b\u0939\u0928\u0932\u093e\u0932 \u0935\u093f\u0936\u094d\u0935\u0928\u093e\u0925\u0928, \u092e\u0928\u094b\u091c \u092c\u093e\u091c\u092a\u0947\u092f\u0940, \u092a\u094d\u0930\u092d\u0941 \u0926\u0947\u0935\u093e, \u0936\u0902\u0915\u0930 \u092e\u0939\u093e\u0926\u0947\u0935\u0928 \u0915\u094b \u092a\u0926\u094d\u092e \u092a\u0941\u0930\u0938\u094d\u0915\u093e\u0930 \u0938\u0947 \u0928\u0935\u093e\u091c\u093e \u0917\u092f\u093e\u0964 \u0935\u0939\u0940\u0902 \u0915\u093e\u0926\u0930 \u0916\u093e\u0928 \u0915\u094b \u092d\u0940 \u092e\u0930\u0923\u094b\u092a\u0930\u093e\u0902\u0924 \u0938\u092e\u094d\u092e\u093e\u0928\u093f\u0924 \u0915\u093f\u092f\u093e \u0917\u092f\u093e\u0964\u092e\u0936\u0939\u0942\u0930 \u0915\u094b\u0930\u093f\u092f\u0949\u0917\u094d\u0930\u092b\u0930 \u092a\u094d\u0930\u092d\u0941 \u0926\u0947\u0935\u093e \u0915\u094b \u092a\u0926\u094d\u092e\u0936\u094d\u0930\u0940 \u0905\u0935\u0949\u0930\u094d\u0921 \u0938\u0947 \u0938\u092e\u094d\u092e\u093e\u0928\u093f\u0924 \u0915\u093f\u092f\u093e \u0917\u092f\u093e\u0964 \u092a\u094d\u0930\u092d\u0941 \u0926\u0947\u0935\u093e \u0915\u0908 \u0939\u093f\u091f \u092b\u093f\u0932\u094d\u092e\u094b\u0902 \u092e\u0947\u0902 \u0905\u092a\u0928\u0947 \u0921\u093e\u0902\u0938 \u0915\u0947 \u0932\u093f\u090f \u091c\u093e\u0928\u0947 \u091c\u093e\u0924\u0947 \u0939\u0948\u0902 \u0914\u0930 \u0935\u0939 \u0915\u0908 \u0935\u0930\u094d\u0937\u094b\u0902 \u0938\u0947 \u092e\u0928\u094b\u0930\u0902\u091c\u0928 \u091c\u0917\