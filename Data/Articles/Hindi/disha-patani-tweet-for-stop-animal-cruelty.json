u0930\u0939\u093e \u0939\u0948\u0964 \u0907\u0938\u092a\u0930 \u0909\u0928\u094d\u200d\u0939\u094b\u0902\u0928\u0947 \u0924\u0938\u094d\u200d\u0935\u0940\u0930 \u0915\u0947 \u0938\u093e\u0925 #Stop Animal Cruelty \u091f\u094d\u0935\u0940\u091f \u0915\u093f\u092f\u093e \u0939\u0948\u0964 \u092c\u0924\u093e \u0926\u0947\u0902 \u0915\u093f \u092c\u0949\u0932\u093f\u0935\u0941\u0921 \u092e\u0947\u0902 \u0905\u092e\u093f\u0924\u093e\u092d \u092c\u091a\u094d\u200d\u091a\u0928, \u0905\u0928\u0941\u0937\u094d\u200d\u0915\u093e \u0936\u0930\u094d\u092e\u093e, \u092b\u0930\u0939\u093e\u0928 \u0905\u0916\u094d\u200d\u0924\u0930, \u091c\u0949\u0928 \u0905\u092c\u094d\u0930\u093e\u0939\u092e, \u0906\u0932\u093f\u092f\u093e \u092d\u091f\u094d\u091f, \u092a\u094d\u0930\u093f\u092f\u0902\u0915\u093e \u091a\u094b\u092a\u0921\u093c\u093e, \u0936\u093e\u0939\u093f\u0926 \u0915\u092a\u0942\u0930, \u0938\u094b\u0928\u092e \u0915\u092a\u0942\u0930, \u0906\u092f\u0936\u093e \u091f\u093e\u0915\u093f\u092f\u093e \u0906\u091c\u092e\u0940, \u0936\u093f\u0932\u094d\u200d\u092a\u093e \u0936\u0947\u091f\u094d\u091f\u0940, \u0932\u093e\u0930\u093e \u0926\u0924\u094d\u200d\u0924\u093e, \u0926\u093f\u092f\u093e \u092e\u093f\u0930\u094d\u091c\u093e \u0914\u0930 \u0905\u0926\u093e \u0936\u0930\u094d\u092e\u093e \u0935\u0928\u094d\u200d\u092f\u091c\u0940\u0935\u094b\u0902 \u0915\u0947 \u0939\u0915 \u0915\u0947 \u0932\u093f\u090f \u0915\u0948\u0902\u092a\u0947\u0928 \u0915\u0930 \u091a\u0941\u0915\u0940 \u0939\u0948\u0902\u0964\u092c\u0949\u0932\u093f\u0935\u0941\u0921 \u0915\u0947 \u0907\u0928 \u0938\u093f\u0924\u093e\u0930\u094b\u0902 \u0915\u094b \u0935\u0928\u094d\u200d\u092f\u091c\u0940\u0935\u094b\u0902 \u0938\u0947 \u0915\u093e\u092b\u0940 \u0932\u0917\u093e\u0935 \u092d\u0940 \u0939\u0948\u0964 \u0907\u0938\u0915\u0947 \u0905\u0932\u093e\u0935\u093e \u092e\u0930\u093e\u0920\u0940 \u0910\u0915\u094d\u200d\u091f\u094d\u0930\u0947\u0938 \u092a\u0942\u091c\u093e \u0938\u093e\u0935\u0902\u0924 \u092d\u0940 \u0905\u0915\u094d\u200d\u0938\u0930 \u0938\u0947\u0935 \u0910\u0928\u093f\u092e\u0932 \u0915\u094b \u0932\u0947\u0915\u0930 \u0932\u094b\u0917\u094b\u0902 \u0938\u0947 \u0905\u092a\u0940\u0932 \u0915\u0930\u0924\u0947 \u0939\u0941\u090f \u0926\u0947\u0916\u0940 \u091c\u093e\u0924\u0940 \u0939\u0948\u0902\u0964"}