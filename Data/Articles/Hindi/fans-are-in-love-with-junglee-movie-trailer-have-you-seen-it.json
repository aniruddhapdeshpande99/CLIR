000\n\u0915\u0940 \u0910\u0938\u0940 \u092b\u093f\u0932\u094d\u092e \u0939\u094b\u0928\u0947 \u091c\u093e \u0930\u0939\u0940 \u0939\u0948 \u091c\u094b \u092b\u0948\u092e\u093f\u0932\u0940 \u0914\u0930 \u092c\u091a\u094d\u091a\u094b\u0902 \u0915\u094b \u092c\u0947\u0939\u0926 \u092a\u0938\u0902\u0926 \u0906\u090f\u0917\u0940\u0964 \u0939\u093e\u0932 \u0939\u0940 \u092e\u0947\u0902 \u092b\u093f\u0932\u094d\u092e \u0915\u093e \u091f\u094d\u0930\u0947\u0932\u0930 \u091c\u093e\u0930\u0940 \u0915\u093f\u092f\u093e \u0917\u092f\u093e \u0939\u0948\u0964 \u0907\u0938 \u092b\u0948\u092e\u093f\u0932\u0940 \u0910\u0915\u094d\u0936\u0928 \u092b\u093f\u0932\u094d\u092e \u0915\u0947 \u091f\u094d\u0930\u0947\u0932\u0930 \u0915\u094b \u092b\u0948\u0902\u0938 \u0916\u0942\u092c \u092a\u0938\u0902\u0926 \u0915\u0930 \u0930\u0939\u0947 \u0939\u0948\u0902\u0964 \u091c\u0902\u0917\u0932\u0940 \u0915\u093e \u091f\u094d\u0930\u0947\u0932\u0930 \u0932\u0949\u0928\u094d\u091a \u0939\u094b\u0928\u0947 \u0915\u0947 \u092c\u093e\u0926 \u0938\u0947 \u0907\u0938\u0947 20 \u092e\u093f\u0932\u093f\u092f\u0928 (2 \u0915\u0930\u094b\u0921\u093c) \u0938\u0947 \u091c\u094d\u092f\u093e\u0926\u093e \u0932\u094b\u0917 \u0926\u0947\u0916 \u091a\u0941\u0915\u0947 \u0939\u0948\u0902\u0964 \u0938\u094b\u0936\u0932 \u092e\u0940\u0921\u093f\u092f\u093e \u092a\u0930 \u092c\u093e\u0924 \u0915\u0930\u0924\u0947 \u0939\u0941\u090f \u092a\u0948\u0930\u0902\u091f\u094d\u0938 \u0907\u0938\u0947 \u092c\u091a\u094d\u091a\u094b\u0902 \u0915\u0947 \u0932\u093f\u090f \u092e\u0938\u094d\u091f \u0935\u0949\u091a \u092e\u0942\u0935\u0940 \u092e\u093e\u0928 \u0930\u0939\u0947 \u0939\u0948\u0902\u0964\u0935\u093f\u0926\u094d\u092f\u0941\u0924 \u091c\u093e\u092e\u0935\u093e\u0932 \u0915\u0947 \u0930\u0942\u092a \u092e\u0947\u0902 \u092c\u091a\u094d\u091a\u094b\u0902 \u0915\u094b \u0907\u0902\u0921\u093f\u092f\u0928 \u091f\u093e\u0930\u094d\u091c\u0928 \u0926\u0947\u0916\u0928\u0947 \u0915\u094b \u092e\u093f\u0932\u0947\u0917\u093e\u0964 \u092b\u093f\u0932\u094d\u092e \u092e\u0947\u0902 \u0910\u0915\u094d\u0936\u0928 \u092d\u0940 \u0910\u0938\u093e \u091c\u094b \u0905\u092c \u0924\u0915 \u0915\u092d\u0940 \u0928\u0939\u0940\u0902 \u0926\u0947\u0916\u093e \u0917\u092f\u093e \u0925\u093e\u0964 \u0907\u0938\u092e\u0947\u0902 \u092a\u094d\u0930\u093e\u091a\u0940\u0928 \u092e\u093e\u0930\u094d\u0936\u0932 \u0906\u0930\u094d\u091f \u0915\u0940 \u0924\u0915\u0928\u0940\u0915 \u0915\u0932\u093e\u0930\u093f\u092a\u092f\u091f\u094d\u091f\u0941 \u0914\u0930 \u090f\u0928\u093f\u092e\u0932 \u092e\u0942\u0935\u092e\u0947\u0902\u091f \u0915\u093e \u0916\u093e\u0938 \u092e\u093f\u0915\u094d\u0938\u091a\u0930 \u0926\u093f\u0916\u0947\u0917\u093e\u0964 \u092c\u093f\u0917 \u0938\u094d\u0915\u094d\u0930\u0940\u0928 \u092a\u0930 \u0915\u0930\u0940\u092c 40 \u0938\u093e\u0932 \u092c\u093e\u0926 \u0905\u0938\u0932\u0940 \u0939\u093e\u0925\u0940 \u0926\u0947\u0916\u0947 \u091c\u093e \u0938\u0915\u0947\u0902\u0917\u0947\u0964 \u092b\u0941\u0932 \u0910\u0915\u094d\u0936\u0928 \u0938\u0947 \u092d\u0930\u092a\u0942\u0930 \u092f\u0939 \u092b\u093f\u0932\u094d\u092e \u091c\u0902\u0917\u0932\u0940 \u091c\u093e\u0928\u0935\u0930\u094b\u0902 \u0938\u0947 \u0907\u0902\u0938\u093e\u0928 \u0915\u0940 \u0926\u094b\u0938\u094d\u0924\u0940 \u0915\u0940 \u092e\u093f\u0938\u093e\u0932 \u0926\u093f\u0916\u093e\u0928\u0947 \u091c\u093e \u0930\u0939\u0940 \u0939\u0948\u0964'\u091c\u0902\u0917\u0932\u0940' \u092b\u093f\u0932\u094d\u092e \u0915\u093e \u0928\u093f\u0930\u094d\u0926\u0947\u0936\u0928 \u0939\u0949\u0932\u093f\u0935\u0941\u0921 \u0915\u0947 \u0921\u093e\u092f\u0930\u0947\u0915\u094d\u091f\u0930 \u091a\u0915 \u0930\u0938\u0932 (\u0926 \u092e\u093e\u0938\u094d\u0915 \u0914\u0930 \u0938\u094d\u0915\u0949\u0930\u094d\u092a\u093f\u092f\u0928 \u0915\u093f\u0902\u0917 \u0915\u0947 \u0932\u093f\u090f \u092e\u0936\u0939\u0942\u0930) \u0915\u0930 \u0930\u0939\u0947 \u0939\u0948\u0902\u0964 \u0907\u0938 \u092b\u093f\u0932\u094d\u092e \u0915\u0940 \u0915\u0939\u093e\u0928\u0940 \u0939\u093e\u0925\u0940 \u0914\u0930 \u0907\u0902\u0938\u093e\u0928 \u0915\u0947 \u0916\u093e\u0938 \u0930\u093f\u0936\u094d\u0924\u0947 \u0915\u0947 \u0907\u0930\u094d\u0926-\u0917\u093f\u0930\u094d\u0926 \u092c\u0941\u0928\u0940 \u0917\u0908 \u0939\u0948\u0964 \u0935\u093f\u0926\u094d\u092f\u0941\u0924 \u091c\u093e\u0928\u0935\u0930\u094b\u0902 \u0915\u0947 \u0921\u0949\u0915\u094d\u091f\u0930 \u0915\u0940 \u092d\u0942\u092e\u093f\u0915\u093e \u092e\u0947\u0902 \u0939\u0948\u0902 \u091c\u094b \u0905\u092a\u0928\u0947 \u092b\u0948\u092e\u093f\u0932\u0940 \u090f\u0932\u093f\u092b\u0947\u0902\u091f \u0930\u093f\u091c\u0930\u094d\u0935 \u092e\u0947\u0902 \u0907\u0902\u091f\u0930\u0928\u0948\u0936\u0928\u0932 \u0936\u093f\u0915\u093e\u0930\u093f\u092f\u094b\u0902 \u0915\u0947 \u0930\u0948\u0915\u0947\u091f \u0915\u093e \u092a\u0924\u093e \u0932\u0917\u093e\u0924\u0947 \u0939\u0948\u0902 \u0914\u0930 \u0909\u0938\u0938\u0947 \u0932\u0921\u093c\u0924\u0947 \u0939\u0948\u0902\u0964 \u091c\u0902\u0917\u0932\u0940 \u092b\u093f\u0932\u094d\u092e\u094d\u0938 \u0915\u0940 '\u091c\u0902\u0917\u0932\u0940' \u0915\u0947 \u092a\u094d\u0930\u0921\u094d\u092f\u0942\u0938\u0930 \u0939\u0948\u0902 \u0935\u093f\u0928\u0940\u0924 \u091c\u0948\u0928 \u0914\u0930 \u0915\u094b-\u092a\u094d\u0930\u0921\u094d\u092f\u0942\u0938\u0930 \u092a\u094d\u0930\u0940\u0924\u093f \u0936\u093e\u0939\u0928\u0940 \u0939\u0948\u0902\u0964 \u092f\u0939 \u092b\u093f\u0932\u094d\u092e 5 \u0905\u092a\u094d\u0930\u0948\u0932 \u0915\u094b \u0930\u093f\u0932\u0940\u091c \u0939\u094b\u0917\u0940\u0964"}