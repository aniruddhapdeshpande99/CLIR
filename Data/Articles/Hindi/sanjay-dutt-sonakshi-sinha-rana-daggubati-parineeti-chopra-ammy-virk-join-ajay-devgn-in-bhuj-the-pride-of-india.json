92e\u0939\u093f\u0932\u093e \u0938\u0941\u0902\u0926\u0930\u092c\u0947\u0928 \u091c\u0947\u0920\u093e \u092e\u0927\u093e\u0930\u092a\u0930\u094d\u092f\u093e \u0915\u0940 \u092d\u0942\u092e\u093f\u0915\u093e \u092e\u0947\u0902 \u0939\u0948\u0902, \u091c\u094b \u0938\u094b\u0936\u0932 \u0935\u0930\u094d\u0915 \u0914\u0930 \u0915\u093f\u0938\u093e\u0928\u0940 \u0915\u093e \u0915\u093e\u092e \u0915\u0930\u0924\u0940 \u0939\u0948\u0964 \u0938\u0941\u0902\u0926\u0930\u092c\u0947\u0928 \u0939\u0940 \u092e\u0927\u093e\u092a\u0941\u0930 \u0915\u0940 299 \u092e\u0939\u093f\u0932\u093e\u0913\u0902 \u0915\u094b \u091c\u0935\u093e\u0928\u094b\u0902 \u0915\u0940 \u092e\u0926\u0926 \u0915\u0947 \u0932\u093f\u090f \u092a\u094d\u0930\u0947\u0930\u093f\u0924 \u0915\u0930\u0924\u0940 \u0939\u0948\u0902\u0964 \u092e\u0939\u093f\u0932\u093e\u0913\u0902 \u0915\u0940 \u0907\u0938\u0940 \u092b\u0948\u091c \u0928\u0947 \u0938\u0947\u0928\u093e \u0915\u0947 \u0932\u093f\u090f \u0930\u0928\u0935\u0947 \u0924\u0948\u092f\u093e\u0930 \u0915\u093f\u092f\u093e \u0925\u093e\u0964 \u0938\u093e\u0909\u0925 \u0915\u0947 \u0938\u094d\u091f\u093e\u0930 \u0930\u093e\u0923\u093e \u0926\u0917\u094d\u0917\u0941\u092c\u0924\u0940 \u0907\u0938 \u092b\u093f\u0932\u094d\u092e \u092e\u0947\u0902 \u092e\u0926\u094d\u0930\u093e\u0938 \u0930\u0947\u091c\u093f\u092e\u0947\u0902\u091f \u0915\u0947 \u0932\u0947\u092b\u094d\u091f\u093f\u0928\u0947\u0902\u091f \u0915\u094b\u0932\u094b\u0928\u0947\u091f \u0915\u0940 \u092d\u0942\u092e\u093f\u0915\u093e \u092e\u0947\u0902 \u0939\u0948\u0902, \u091c\u094b \u0935\u093f\u0918\u093e\u0915\u094b\u091f \u091a\u094c\u0915\u0940 \u092e\u0947\u0902 \u092a\u094b\u0938\u094d\u091f\u0947\u0921 \u0939\u0948\u0902 \u0914\u0930 \u092d\u093e\u0930\u0924-\u092a\u093e\u0915\u093f\u0938\u094d\u0924\u093e\u0928 \u0915\u0947 1971 \u0915\u0940 \u0932\u095c\u093e\u0908 \u092e\u0947\u0902 \u092d\u0940 \u0936\u093e\u092e\u093f\u0932 \u0930\u0939\u0924\u0947 \u0939\u0948\u0902\u0964\u092a\u0930\u093f\u0923\u0940\u0924\u093f \u091a\u094b\u092a\u095c\u093e \u092b\u093f\u0932\u094d\u092e \u092e\u0947\u0902 \u090f\u0915 \u092d\u093e\u0930\u0924\u0940\u092f \u091c\u093e\u0938\u0942\u0938 \u0939\u093f\u0928\u093e \u0930\u0939\u092e\u093e\u0928 \u0915\u0940 \u092d\u0942\u092e\u093f\u0915\u093e \u092e\u0947\u0902 \u0939\u0948\u0902, \u091c\u094b \u092a\u093e\u0915\u093f\u0938\u094d\u0924\u093e\u0928 \u0915\u0947 \u0932\u093e\u0939\u094c\u0930 \u092e\u0947\u0902 \u0930\u0939\u0924\u0940 \u0939\u0948\u0902\u0964 \u092a\u0902\u091c\u093e\u092c\u0940 \u0910\u0915\u094d\u091f\u0930 \u0910\u092e\u0940 \u0935\u093f\u0930\u094d\u0915 \u090f\u0915 \u092b\u093e\u0907\u091f\u0930 \u092a\u0949\u092f\u0932\u091f \u0915\u0940 \u092e\u0939\u0924\u094d\u0935\u092a\u0942\u0930\u094d\u0923 \u092d\u0942\u092e\u093f\u0915\u093e \u092e\u0947\u0902 \u0906\u090f\u0902\u0917\u0947\u0964\u0907\u0938 \u092b\u093f\u0932\u094d\u092e \u0915\u093e \u0928\u093f\u0930\u094d\u092e\u093e\u0923 \u091f\u0940-\u0938\u0940\u0930\u093f\u091c \u0914\u0930 \u0938\u093f\u0932\u0947\u0915\u094d\u091f \u092e\u0940\u0921\u093f\u092f\u093e \u0939\u094b\u0932\u094d\u0921\u093f\u0902\u0917\u094d\u0938 \u090f\u0932\u090f\u0932\u092a\u0940 \u0915\u0930 \u0930\u0939\u0947 \u0939\u0948\u0902\u0964 '\u092d\u0941\u091c: \u0926 \u092a\u094d\u0930\u093e\u0907\u0921 \u0911\u092b \u0907\u0902\u0921\u093f\u092f\u093e', 1971 \u0915\u0947 \u092f\u0941\u0926\u094d\u0927 \u092e\u0947\u0902 \u0939\u0941\u0908 \u090f\u0915 \u0938\u0924\u094d\u092f \u0918\u091f\u0928\u093e \u092a\u0930 \u0906\u0927\u093e\u0930\u093f\u0924 \u0939\u0948\u0964 \u0905\u091c\u092f \u0907\u0938\u092e\u0947\u0902 \u0938\u094d\u0915\u094d\u0935\u093e\u0921\u094d\u0930\u0928 \u0932\u0940\u0921\u0930 \u0935\u093f\u091c\u092f \u0915\u0930\u094d\u0923\u093f\u0915 \u0915\u093e \u0915\u093f\u0930\u0926\u093e\u0930 \u0928\u093f\u092d\u093e\u090f\u0902\u0917\u0947, \u091c\u094b 1971 \u092e\u0947\u0902 \u092d\u093e\u0930\u0924-\u092a\u093e\u0915\u093f\u0938\u094d\u0924\u093e\u0928 \u0915\u0947 \u092c\u0940\u091a \u0939\u0941\u090f \u092f\u0941\u0926\u094d\u0927 \u092e\u0947\u0902 \u092d\u0941\u091c \u090f\u0905\u0930\u092a\u094b\u0930\u094d\u091f \u0915\u0947 \u0907\u0902\u091a\u093e\u0930\u094d\u091c \u0925\u0947\u0964 \u0915\u0930\u094d\u0923\u093f\u0915 \u0914\u0930 \u0909\u0928\u0915\u0940 \u091f\u0940\u092e \u0928\u0947 \u092e\u0939\u093f\u0932\u093e\u0913\u0902 \u0915\u0940 \u0938\u0939\u093e\u092f\u0924\u093e \u0938\u0947 \u092d\u0941\u091c \u092e\u0947\u0902 \u0928\u0937\u094d\u091f \u0939\u094b \u0917\u0908 \u090f\u092f\u0930\u0938\u094d\u091f\u094d\u0930\u093f\u092a \u092b\u093f\u0930 \u0938\u0947 \u092c\u0928\u093e\u0908 \u0925\u0940\u0964 \u0907\u0938\u0947 \u092d\u093e\u0930\u0924 \u0915\u093e '\u092a\u0930\u094d\u0932 \u0939\u0949\u0930\u094d\u092c\u0930 \u092e\u094b\u092e\u0947\u0902\u091f' \u0915\u0939\u093e \u0917\u092f\u093e \u0939\u0948\u0964\u0907\u0938 \u090f\u092f\u0930\u0938\u094d\u091f\u094d\u0930\u093f\u092a \u0915\u094b \u092a\u093e\u0915\u093f\u0938\u094d\u0924\u093e\u0928 \u0928\u0947 \u092c\u092e \u092b\u0947\u0902\u0915 \u0915\u0930 \u0928\u0937\u094d\u091f \u0915\u0930 \u0926\u093f\u092f\u093e \u0925\u093e\u0964 \u0915\u0930\u094d\u0923\u093f\u0915 \u0928\u0947 \u0928\u093f\u0915\u091f \u0938\u094d\u0925\u093f\u0924 \u0917\u093e\u0902\u0935 \u0915\u0940 300 \u092e\u0939\u093f\u0932\u093e\u0913\u0902 \u0915\u094b \u0915\u093e\u092e \u0915\u0930\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u0930\u093e\u091c\u0940 \u0915\u093f\u092f\u093e \u0925\u093e \u0914\u0930 \u0909\u0928 \u092e\u0939\u093f\u0932\u093e\u0913\u0902 \u0928\u0947 \u090f\u092f\u0930\u0938\u094d\u091f\u094d\u0930\u093f\u092a \u092b\u093f\u0930 \u092c\u0928\u093e \u0926\u0940 \u0925\u0940, \u091c\u093f\u0938\u0938\u0947 \u0907\u0902\u0921\u093f\u092f\u0928 \u0906\u0930\u094d\u092e\u0940 \u0915\u0947 \u0911\u092b\u093f\u0938\u0930\u094d\u0938 \u0938\u0941\u0930\u0915\u094d\u0937\u093f\u0924 \u0932\u0948\u0902\u0921 \u0915\u0930 \u0938\u0915\u0947\u0902\u0964 \u0915\u0930\u094d\u0923\u093f\u0915 \u0928\u0947 \u0926\u094b \u0905\u0928\u094d\u092f \u0911\u092b\u093f\u0938\u0930\u094d\u0938, 50 \u090f\u092f\u0930\u092b\u094b\u0930\u094d\u0938 \u0938\u0948\u0928\u093f\u0915 \u0914\u0930 60 \u0921\u093f\u092b\u0947\u0902\u0938 \u0938\u093f\u0915\u094d\u092f\u0941\u0930\u093f\u091f\u0940 \u0915\u0949\u0930\u094d\u092a\u094d\u0938 \u0915\u0947 \u0938\u093e\u0925 \u092f\u0939 \u0915\u093e\u092e \u0915\u093f\u092f\u093e \u0925\u093e\u0964\u091f\u0940-\u0938\u0940\u0930\u093f\u091c \u0915\u0947 \u092d\u0942\u0937\u0923 \u0915\u0941\u092e\u093e\u0930 \u0915\u093e \u0915\u0939\u0928\u093e \u0939\u0948, '\u0907\u0938 \u092c\u0939\u093e\u0926\u0941\u0930\u0940 \u092d\u0930\u0940 \u0915\u0939\u093e\u0928\u0940 \u092a\u0930 \u0939\u092e\u0928\u0947 \u092b\u093f\u0932\u094d\u092e \u0907\u0938\u0932\u093f\u090f \u092c\u0928\u093e\u0928\u0947 \u0915\u093e \u092b\u0948\u0938\u0932\u093e \u0932\u093f\u092f\u093e \u0939\u0948, \u0924\u093e\u0915\u093f \u0906\u0928\u0947 \u0935\u093e\u0932\u0940 \u092a\u0940\u0922\u093c\u093f\u092f\u093e\u0902 \u0907\u0938 \u092c\u0939\u093e\u0926\u0941\u0930 \u0938\u0948\u0928\u093f\u0915 \u0915\u0947 \u092c\u093e\u0930\u0947 \u092e\u0947\u0902 \u091c\u093e\u0928 \u0938\u0915\u0947\u0902\u0964'\u0938\u094d\u0915\u094d\u0935\u093e\u0921\u094d\u0930\u0928 \u0932\u0940\u0921\u0930 \u0935\u093f\u091c\u092f \u0915\u0930\u094d\u0923\u093f\u0915 \u0915\u093e \u0915\u0939\u0928\u093e \u0939\u0948, '\u0939\u092e \u0909\u0938 \u0938\u092e\u092f \u092f\u0941\u0926\u094d\u0927 \u0932\u0921\u093c \u0930\u0939\u0947 \u0925\u0947 \u0914\u0930 \u0909\u0938 \u0926\u094c\u0930\u093e\u0928 \u092f\u0926\u093f \u0915\u093f\u0938\u0940 \u092e\u0939\u093f\u0932\u093e \u0915\u0947 \u0938\u093e\u0925 \u0915\u0941\u091b \u0905\u0928\u0939\u094b\u0928\u0940 \u0939\u094b \u091c\u093e\u0924\u0940 \u0924\u094b \u092f\u0939 \u0939\u092e\u093e\u0930\u0947 \u0932\u093f\u090f \u092d\u093e\u0930\u0940 \u0928\u0941\u0915\u0938\u093e\u0928 \u0939\u094b \u0938\u0915\u0924\u093e \u0925\u093e, \u0932\u0947\u0915\u093f\u0928 \u092e\u0948\u0902\u0928\u0947 \u0938\u093e\u0939\u0938\u093f\u0915 \u092b\u0948\u0938\u0932\u093e \u0932\u093f\u092f\u093e \u0914\u0930 \u0915\u093e\u092e \u0936\u0941\u0930\u0942 \u0915\u093f\u092f\u093e\u0964 \u092e\u0948\u0902\u0928\u0947 \u0909\u0928\u094d\u0939\u0947\u0902 \u0938\u0902\u0915\u094d\u0937\u0947\u092a \u092e\u0947\u0902 \u0938\u092e\u091d\u093e\u092f\u093e \u0915\u093f \u092f\u0926\u093f \u0939\u092e\u0932\u093e \u0939\u094b\u0924\u093e \u0939\u0948 \u0924\u094b \u0909\u0928\u094d\u0939\u0947\u0902 \u0915\u093f\u0938 \u0924\u0930\u0939 \u092c\u091a\u0928\u093e \u0939\u0948\u0964 \u0909\u0928\u094d\u0939\u094b\u0902\u0928\u0947 \u092f\u0939 \u0915\u093e\u092e \u092c\u0939\u093e\u0926\u0941\u0930\u0940 \u092a\u0942\u0930\u094d\u0935\u0915 \u0915\u093f\u092f\u093e\u0964 \u092e\u0947\u0930\u093e \u092e\u093e\u0928\u0928\u093e \u0939\u0948 \u0915\u093f \u092e\u0947\u0930\u093e \u0915\u093f\u0930\u0926\u093e\u0930 \u0915\u0947\u0935\u0932 \u0905\u091c\u092f \u0926\u0947\u0935\u0917\u0928 \u0939\u0940 \u0928\u093f\u092d\u093e \u0938\u0915\u0924\u0947 \u0939\u0948\u0902 \u0914\u0930 \u092e\u0941\u091d\u0947 \u0916\u0941\u0936\u0940 \u0939\u0941\u0908 \u0939\u0948 \u0915\u093f \u0935\u0939 \u092b\u093f\u0932\u094d\u092e \u0915\u0930 \u0930\u0939\u0947 \u0939\u0948\u0902\u0964'\u0907\u0928 \u0926\u093f\u0928\u094b\u0902 \u0905\u091c\u092f \u0926\u0947\u0935\u0917\u0928 \u0905\u092a\u0928\u0947 \u092a\u094d\u0930\u0949\u0921\u0915\u094d\u0936\u0928 \u092e\u0947\u0902 '\u0924\u093e\u0928\u093e\u091c\u0940: \u0926 \u0905\u0928\u0938\u0902\u0917 \u0935\u0949\u0930\u093f\u092f\u0930' \u0928\u093e\u092e \u0915\u0940 \u092b\u093f\u0932\u094d\u092e \u0915\u093e \u0928\u093f\u0930\u094d\u092e\u093e\u0923 \u0915\u0930 \u0930\u0939\u0947 \u0939\u0948\u0902\u0964 \u0905\u091c\u092f \u0926\u0947\u0935\u0917\u0928 \u0915\u0947 \u0938\u093e\u0925 \u0907\u0938 \u092b\u093f\u0932\u094d\u092e \u092e\u0947\u0902 \u0938\u0948\u092b \u0905\u0932\u0940 \u0916\u093e\u0928, \u0915\u093e\u091c\u094b\u0932 \u0914\u0930 \u092a\u0902\u0915\u091c \u0924\u094d\u0930\u093f\u092a\u093e\u0920\u0940 \u0938\u0939\u093f\u0924 \u0915\u0908 \u0914\u0930 \u0938\u093f\u0924\u093e\u0930\u0947 \u0928\u091c\u0930 \u0906\u090f\u0902\u0917\u0947\u0964 \u092b\u093f\u0932\u094d\u092e \u0915\u0940 \u0936\u0942\u091f\u093f\u0902\u0917 \u092a\u0942\u0930\u0940 \u0939\u094b \u091a\u0941\u0915\u0940 \u0939\u0948\u0964 \u092f\u0939 \u092b\u093f\u0932\u094d\u092e 27 \u0926\u093f\u0938\u0902\u092c\u0930 \u0915\u094b \u0907\u0938\u0940 \u0938\u093e\u0932 \u0926\u0947\u0936\u092d\u0930 \u092e\u0947\u0902 \u0930\u093f\u0932\u0940\u095b \u0939\u094b\u0917\u0940\u0964 \u0907\u0938 \u092b\u093f\u0932\u094d\u092e \u0915\u094b \u0938\u0902\u0917\u0940\u0924 \u0938\u0947 \u0905\u091c\u092f-\u0905\u0924\u0941\u0932 \u0915\u0940 \u091c\u094b\u095c\u0940 \u0938\u091c\u093e \u0930\u0939\u0940 \u0939\u0948\u0964"}