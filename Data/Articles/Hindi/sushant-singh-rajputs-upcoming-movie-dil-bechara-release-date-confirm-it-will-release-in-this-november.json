\u2014 Fox Star Hindi (@foxstarhindi) 1551963428000\n\u0915\u0940 \u0906\u0928\u0947 \u0935\u093e\u0932\u0940 \u092b\u093f\u0932\u094d\u092e \u0926\u093f\u0932 \u092c\u0947\u091a\u093e\u0930\u093e \u0915\u0940 \u0930\u093f\u0932\u0940\u091c \u0921\u0947\u091f \u0915\u093e \u0916\u0941\u0932\u093e\u0938\u093e \u0939\u094b \u0917\u092f\u093e \u0939\u0948\u0964 \u092b\u093f\u0932\u094d\u092e \u0907\u0938\u0940 \u0938\u093e\u0932 \u0915\u0947 \u0905\u0902\u0924 \u092e\u0947\u0902 \u0930\u093f\u0932\u0940\u091c \u0939\u094b\u0917\u0940\u0964 \u092b\u093f\u0932\u094d\u092e \u0915\u094b \u0938\u093f\u0932\u094d\u0935\u0930 \u0938\u094d\u0915\u094d\u0930\u0940\u0928 \u092a\u0930 \u0909\u0924\u093e\u0930\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u092a\u094d\u0930\u0921\u094d\u092f\u0942\u0938\u0930\u094d\u0938 \u0928\u0947 29 \u0928\u0935\u0902\u092c\u0930 \u0915\u094b \u091a\u0941\u0928\u093e \u0939\u0948\u0964 \u0916\u093e\u0938 \u092c\u093e\u0924 \u092f\u0939 \u0939\u0948 \u0915\u093f \u0909\u0938\u0938\u0947 \u090f\u0915 \u0939\u092b\u094d\u0924\u0947 \u092a\u0939\u0932\u0947 \u092f\u093e\u0928\u0940 22 \u0928\u0935\u0902\u092c\u0930 \u0915\u094b \u0939\u0940 \u0905\u091c\u092f \u0926\u0947\u0935\u0917\u0928 \u0915\u0940 \u092a\u094d\u0930\u0924\u093f\u0915\u094d\u0937\u093f\u0924 \u092b\u093f\u0932\u094d\u092e \u0924\u093e\u0928\u093e\u091c\u0940 \u092d\u0940 \u0930\u093f\u0932\u0940\u091c \u0939\u094b\u0917\u0940\u0964 \u092c\u093e\u0924 \u0915\u0930\u0947\u0902 \u0926\u093f\u0932 \u092c\u0947\u091a\u093e\u0930\u093e \u092b\u093f\u0932\u094d\u092e \u0915\u0940 \u0924\u094b \u0907\u0938\u092e\u0947\u0902 \u0938\u0941\u0936\u093e\u0902\u0924 \u0938\u093f\u0902\u0939 \u0930\u093e\u091c\u092a\u0942\u0924 \u0932\u0940\u0921 \u0930\u094b\u0932 \u092e\u0947\u0902 \u0939\u0948\u0902\u0964\u0909\u0928\u0915\u093e \u0938\u093e\u0925 \u0926\u0947\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u092b\u093f\u0932\u094d\u092e \u092e\u0947\u0902 \u0938\u0902\u091c\u0928\u093e \u0938\u093e\u0902\u0918\u0940 \u092d\u0940 \u0926\u093f\u0916\u0947\u0902\u0917\u0940\u0964 \u0938\u0941\u0936\u093e\u0902\u0924 \u0915\u0947 \u0905\u0932\u093e\u0935\u093e \u092b\u093f\u0932\u094d\u092e \u092e\u0947\u0902 \u0938\u0948\u092b \u0905\u0932\u0940 \u0916\u093e\u0928 \u0914\u0930 \u092e\u093f\u0932\u093f\u0902\u0926 \u0938\u094b\u092e\u0928 \u092d\u0940 \u0926\u093f\u0916\u0947\u0902\u0917\u0947\u0964 \u0907\u0938\u0938\u0947 \u092a\u0939\u0932\u0947 \u0938\u0941\u0936\u093e\u0902\u0924 \u0938\u093f\u0902\u0939 \u0930\u093e\u091c\u092a\u0942\u0924 \u0928\u0947 \u092b\u093f\u0932\u094d\u092e \u0915\u0947 \u092c\u093e\u0930\u0947 \u092e\u0947\u0902 \u092c\u093e\u0924 \u0915\u0930\u0924\u0947 \u0939\u0941\u090f \u092c\u0924\u093e\u092f\u093e \u0925\u093e \u0915\u093f \u0926\u093f\u0932 \u092c\u0947\u091a\u093e\u0930\u093e \u092b\u093f\u0932\u094d\u092e \u0915\u094b \u092a\u0939\u0932\u0947 '\u0915\u093f\u091c\u0940 \u0914\u0930 \u092e\u0948\u0928\u0940' \u0915\u0947 \u0928\u093e\u092e \u0938\u0947 \u092c\u0928\u093e\u092f\u093e \u091c\u093e \u0930\u0939\u093e \u0925\u093e\u0964 \u092c\u093e\u0926 \u092e\u0947\u0902 \u0907\u0938\u0947 \u092c\u0926\u0932\u0915\u0930 \u0926\u093f\u0932 \u092c\u0947\u091a\u093e\u0930\u093e \u0930\u0916\u093e \u0917\u092f\u093e\u0964 \u092f\u0939 \u091c\u0949\u0936 \u092c\u094b\u0928\u0940 \u0915\u0940 \u092b\u093f\u0932\u094d\u200d\u092e \u0915\u093e \u0939\u093f\u0902\u0926\u0940 \u0935\u0930\u094d\u091c\u0928 \u0939\u0948\u0964 \u0907\u0938 \u092b\u093f\u0932\u094d\u200d\u092e \u0915\u094b \u092e\u0941\u0915\u0947\u0936 \u091b\u093e\u092c\u0921\u093c\u093e \u0921\u093e\u092f\u0930\u0947\u0915\u094d\u200d\u091f \u0915\u0930 \u0930\u0939\u0947 \u0939\u0948\u0902\u0964\u092e\u093e\u0932\u0942\u092e \u0939\u094b, \u0939\u093e\u0932 \u0939\u0940 \u092e\u0947\u0902 \u0938\u0941\u0936\u093e\u0902\u0924 \u0938\u093f\u0902\u0939 \u0930\u093e\u091c\u092a\u0942\u0924 \u0938\u094b\u0928\u091a\u093f\u0930\u0948\u092f\u093e \u0914\u0930 \u0915\u0947\u0926\u093e\u0930\u0928\u093e\u0925 \u092b\u093f\u0932\u094d\u092e \u092e\u0947\u0902 \u092c\u0921\u093c\u0947 \u092a\u0930\u094d\u0926 \u092a\u0930 \u0926\u093f\u0916\u0947 \u0925\u0947\u0964 \u0915\u0947\u0926\u093e\u0930\u0928\u093e\u0925 \u092e\u0947\u0902 \u0909\u0928\u0915\u0940 \u0910\u0915\u094d\u091f\u093f\u0902\u0917 \u0928\u0947 \u0939\u0930 \u0915\u093f\u0938\u0940 \u0915\u094b \u0926\u0940\u0935\u093e\u0928\u093e \u092c\u0928\u093e \u0926\u093f\u092f\u093e\u0964 \u0938\u094b\u0928 \u091a\u093f\u0930\u0948\u092f\u093e \u092b\u093f\u0932\u094d\u092e \u092e\u0947\u0902 \u0909\u0928\u0915\u0947 \u0938\u093e\u0925 \u0930\u0923\u0935\u0940\u0930 \u0936\u094c\u0930\u0940, \u092e\u0928\u094b\u091c \u092c\u093e\u091c\u092a\u0947\u092f\u0940 \u0914\u0930 \u092d\u0942\u092e\u093f \u092a\u0947\u0921\u0928\u0947\u0915\u0930 \u091c\u0948\u0938\u0947 \u091c\u093e\u0928\u0947\u092e\u093e\u0928\u0947 \u0910\u0915\u094d\u091f\u0930 \u0925\u0947\u0964 \u0939\u093e\u0932\u093e\u0902\u0915\u093f \u092b\u093f\u0932\u094d\u092e \u092c\u0949\u0915\u094d\u0938 \u0911\u092b\u093f\u0938 \u092a\u0930 \u0915\u0941\u091b \u0916\u093e\u0938 \u0915\u092e\u093e\u0932 \u0928\u0939\u0940\u0902 \u0915\u0930 \u092a\u093e\u0908\u0964 \u0926\u0947\u0916\u0928\u093e \u0939\u094b\u0917\u093e \u0915\u093f \u0938\u0941\u0936\u093e\u0902\u0924 \u0915\u0940 \u0906\u0928\u0947 \u0935\u093e\u0932\u0940 \u092b\u093f\u0932\u094d\u092e \u0926\u093f\u0932 \u092c\u0947\u091a\u093e\u0930\u093e \u0905\u091c\u092f \u0926\u0947\u0935\u0917\u0928 \u0915\u0940 '\u0924\u093e\u0928\u093e\u091c\u0940' \u0915\u094b \u091f\u0915\u094d\u0915\u0930 \u0926\u0947 \u092a\u093e\u090f\u0917\u0940 \u092f\u093e \u0928\u0939\u0940\u0902!"}