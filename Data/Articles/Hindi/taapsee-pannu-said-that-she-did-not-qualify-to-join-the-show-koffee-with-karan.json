0902 \u091c\u092c \u0915\u092e\u0940\u0921\u093f\u092f\u0928 \u0935\u0940\u0930 \u0926\u093e\u0938 \u0928\u0947 \u091f\u0940\u0935\u0940 \u0936\u094b ' \u0915\u0949\u092b\u0940 \u0935\u093f\u0926 \u0915\u0930\u0923 ' \u0915\u094b \u0932\u0947\u0915\u0930 \u0924\u093e\u092a\u0938\u0940 \u0915\u094b \u090f\u0915 \u091f\u094d\u0935\u0940\u091f \u0915\u093f\u092f\u093e \u0924\u094b \u0924\u093e\u092a\u0938\u0940 \u0928\u0947 \u092d\u0940 \u0909\u0928\u094d\u200d\u0939\u0947\u0902 \u0905\u092a\u0928\u0947 \u0939\u0940 \u0905\u0902\u0926\u093e\u091c \u092e\u0947\u0902 \u091c\u0935\u093e\u092c \u0926\u093f\u092f\u093e\u0964 \u0907\u0938\u0915\u0947 \u092c\u093e\u0926 \u0924\u094b \u091f\u094d\u0935\u0940\u091f\u0930 \u092a\u0930 \u092e\u0940\u0921\u093f\u092f\u093e \u0915\u0947 \u0905\u0932\u093e\u0935\u093e \u0924\u093e\u092a\u0938\u0940 \u0915\u0947 \u092b\u0948\u0902\u0938 \u0928\u0947 \u092d\u0940 \u0936\u094b \u0915\u0947 \u0939\u094b\u0938\u094d\u200d\u091f \u0915\u0930\u0923 \u091c\u094c\u0939\u0930 \u0915\u094b \u0932\u0947\u0915\u0930 \u0924\u092e\u093e\u092e \u091f\u094d\u0935\u0940\u091f\u0938 \u0915\u093f\u092f\u0947\u0964\u0932\u094b\u0917\u094b\u0902 \u0928\u0947 \u091c\u0939\u093e\u0902 \u0924\u093e\u092a\u0938\u0940 \u0915\u094b \u0936\u093e\u0928\u0926\u093e\u0930 \u0910\u0915\u094d\u200d\u091f\u094d\u0930\u0947\u0938 \u092c\u0924\u093e\u092f\u093e\u0964 \u0935\u0939\u0940\u0902 \u092f\u0939 \u092d\u0940 \u0915\u0939\u093e \u0915\u093f \u0909\u0928\u094d\u200d\u0939\u0947\u0902 \u0915\u093f\u0938\u0940 \u0915\u0947 \u0915\u0949\u092b\u0940 \u0936\u094b \u092a\u0930 \u091c\u093e\u0928\u0947 \u0915\u0940 \u091c\u0930\u0942\u0930\u0924 \u0928\u0939\u0940\u0902 \u0939\u0948\u0964\u092c\u0924\u093e \u0926\u0947\u0902 \u0915\u093f \u0939\u093e\u0932 \u0939\u0940 \u092e\u0947\u0902 \u091c\u092c \u091f\u0940\u0935\u0940 \u0936\u094b '\u0915\u0949\u092b\u0940 \u0935\u093f\u0926 \u0915\u0930\u0923' \u0915\u0947 \u0906\u0916\u093f\u0930\u0940 \u0910\u092a\u093f\u0938\u094b\u0921 '\u0915\u0949\u092b\u0940 \u0905\u0935\u0949\u0930\u094d\u0921 \u0928\u093e\u0907\u091f' \u0915\u094b \u0936\u0942\u091f \u0915\u093f\u092f\u093e \u0917\u092f\u093e \u0924\u094b \u0909\u0938\u092e\u0947\u0902 \u0915\u093f\u0930\u0923 \u0916\u0947\u0930, \u092e\u0932\u093e\u0907\u0915\u093e \u0905\u0930\u094b\u0921\u093c\u093e, \u0935\u0940\u0930 \u0926\u093e\u0938 \u0914\u0930 \u092e\u0932\u094d\u0932\u093f\u0915\u093e \u0926\u0941\u0906 \u0915\u094b \u0906\u0902\u092e\u0924\u094d\u0930\u093f\u0924 \u0915\u093f\u092f\u093e \u0917\u092f\u093e\u0964 \u0907\u0938\u0915\u0947 \u092c\u093e\u0926 \u0915\u092e\u0940\u0921\u093f\u092f\u0928 \u0935\u0940\u0930 \u0926\u093e\u0938 \u0928\u0947 \u0905\u092a\u0928\u0947 \u0913\u092a\u0928\u093f\u0902\u0917 \u0910\u0915\u094d\u200d\u091f \u0915\u093e \u091f\u0940\u091c\u0930 \u091f\u094d\u0935\u0940\u091f \u0915\u0930\u0924\u0947 \u0939\u0941\u090f \u0932\u093f\u0916\u093e '\u0907\u0938 \u0938\u0947\u091f \u092a\u0930 \u0907\u0924\u0928\u093e \u092a\u093f\u0902\u0915 \u0939\u0948 \u0915\u093f \u0924\u093e\u092a\u0938\u0940 \u0928\u0947 \u0907\u0938\u092a\u0930 \u0910\u0915\u094d\u091f\u093f\u0902\u0917 \u0915\u0930\u0928\u0947 \u0915\u0940 \u0915\u094b\u0936\u093f\u0936 \u0915\u0940\u0964'\u0935\u0940\u0930 \u0926\u093e\u0938 \u0915\u0947 \u0907\u0938 \u091f\u094d\u0935\u0940\u091f \u092a\u0930 \u0924\u093e\u092a\u0938\u0940 \u0928\u0947 \u092d\u0940 \u092c\u0947\u0939\u0924\u0930\u0940\u0928 \u0905\u0902\u0926\u093e\u091c \u092e\u0947\u0902 \u091c\u0935\u093e\u092c \u0926\u093f\u092f\u093e\u0964 \u0909\u0928\u094d\u200d\u0939\u094b\u0902\u0928\u0947 \u0936\u094b \u0915\u0947 \u0939\u094b\u0938\u094d\u200d\u091f \u092a\u0930 \u0924\u0902\u091c \u0915\u0938\u0924\u0947 \u0939\u0941\u090f \u0932\u093f\u0916\u093e '\u0939\u093e\u0939\u093e\u0939\u093e\u0939\u093e, \u0905\u091a\u094d\u200d\u091b\u093e \u0939\u0948, \u0932\u0947\u0915\u093f\u0928 \u0924\u093e\u092a\u0938\u0940 \u0935\u0939\u093e\u0902 \u0924\u0915 \u092a\u0939\u0941\u0902\u091a\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u0915\u094d\u200d\u0935\u0949\u0932\u0940\u092b\u093e\u0908 \u0928\u0939\u0940\u0902 \u0915\u0930 \u092a\u093e\u0908,\u0964' \u0909\u0928\u094d\u200d\u0939\u094b\u0902\u0928\u0947 \u0932\u093f\u0916\u093e ' \u092e\u0948\u0902 \u092f\u0939\u0940\u0902 \u0938\u0947 \u0906\u092a \u0932\u094b\u0917\u094b\u0902 \u0915\u094b \u0926\u0947\u0916\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u0909\u0924\u094d\u200d\u0938\u0941\u0915 \u0939\u0942\u0902, \u0906\u092a \u0938\u092d\u0940 \u0915\u093e\u092b\u0940 \u091c\u094d\u200d\u092f\u093e\u0926\u093e \u0916\u0941\u0936 \u0928\u091c\u0930 \u0906 \u0930\u0939\u0947 \u0939\u0948\u0902\u0964'\u0907\u0938\u0915\u0947 \u092c\u093e\u0926 \u0935\u0940\u0930 \u0928\u0947 \u0909\u0928\u094d\u200d\u0939\u0947\u0902 \u0930\u093f\u092a\u094d\u200d\u0932\u093e\u0908 \u0915\u093f\u092f\u093e \u0915\u093f \u0935\u0939 \u0915\u093e\u092b\u0940 \u092a\u094d\u0930\u0924\u093f\u092d\u093e\u0936\u093e\u0932\u0940 \u0939\u0948\u0902 \u0914\u0930 \u0935\u0939\u093e\u0902 \u092a\u0939\u0941\u0902\u091a\u0928\u0947 \u0915\u0940 \u0938\u093e\u0930\u0940 \u092f\u094b\u0917\u094d\u200d\u0924\u093e\u090f\u0902 \u092d\u0940 \u0909\u0928\u092e\u0947 \u0939\u0948\u0902\u0964 \u0907\u0938\u092a\u0930 \u0924\u093e\u092a\u0938\u0940 \u0928\u0947 \u092d\u0940 '\u0905\u0930\u0947 \u092e\u0948\u0902 \u0924\u094b \u090f\u0915 \u0915\u094d\u0930\u0947\u091c\u0940 \u092b\u0928\u0940 \u0930\u093f\u0910\u0915\u094d\u200d\u0936\u0928 \u0915\u0940 \u0909\u092e\u094d\u200d\u092e\u0940\u0926 \u0915\u0930 \u0930\u0939\u0940 \u0925\u0940 # Fanrequest' \u0932\u093f\u0916\u0915\u0930 \u091c\u0935\u093e\u092c \u0926\u093f\u092f\u093e\u0964 \u0939\u093e\u0932\u093e\u0902\u0915\u093f \u092f\u0939 \u092c\u093e\u0924 \u092f\u0939\u0940\u0902 \u0916\u0924\u094d\u200d\u092e \u0928\u0939\u0940\u0902 \u0939\u0941\u0908, \u0906\u0917\u0947 \u092d\u0940 \u0907\u0938 \u092a\u0930 \u0915\u0908 \u0932\u094b\u0917\u094b\u0