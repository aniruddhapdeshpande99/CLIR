u0939\u0948 \u0915\u093f \u092b\u093f\u0932\u094d\u092e '\u0938\u093f\u092e\u0930\u0928' \u092c\u0928\u0928\u0947 \u0915\u0947 \u0926\u094c\u0930\u093e\u0928 \u0935\u0939 \u092d\u0940 \u0909\u0938\u0940 \u0938\u094d\u0925\u093f\u0924\u093f \u092e\u0947\u0902 \u0925\u0947 \u091c\u093f\u0938\u092e\u0947\u0902 '\u092e\u0923\u093f\u0915\u0930\u094d\u0923\u093f\u0915\u093e' \u0915\u0947 \u0926\u094c\u0930\u093e\u0928 \u0915\u0943\u0937 \u0925\u0947\u0964 \u092c\u0924\u093e \u0926\u0947\u0902 \u0915\u093f \u0915\u0902\u0917\u0928\u093e \u0915\u094b \u0905\u092a\u0942\u0930\u094d\u0935 \u0915\u0940 \u092e\u0930\u094d\u091c\u0940 \u0915\u0947 \u0916\u093f\u0932\u093e\u092b \u092b\u093f\u0932\u094d\u092e '\u0938\u093f\u092e\u0930\u0928' \u0915\u0947 \u0932\u093f\u090f \u0915\u094b-\u0930\u093e\u0907\u091f\u0930 \u0915\u093e \u0915\u094d\u0930\u0947\u0921\u093f\u091f \u0926\u093f\u092f\u093e \u0917\u092f\u093e \u0925\u093e\u0964 \u091f\u094d\u0935\u093f\u091f\u0930 \u092a\u0930 \u0905\u092a\u0928\u0940 \u0932\u0947\u091f\u0947\u0938\u094d\u091f \u092a\u094b\u0938\u094d\u091f \u092e\u0947\u0902 \u0905\u092a\u0942\u0930\u094d\u0935 \u0928\u0947 \u0915\u0902\u0917\u0928\u093e \u0915\u094b '\u0926\u0941\u0937\u094d\u091f \u0936\u0915\u094d\u0924\u093f' \u0915\u0939\u093e \u0939\u0948 \u0914\u0930 \u092e\u0940\u0921\u093f\u092f\u093e \u0915\u094b \u092d\u0940 '\u092e\u0923\u093f\u0915\u0930\u094d\u0923\u093f\u0915\u093e' \u0915\u094b \u0939\u093f\u091f \u092c\u0924\u093e\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u0906\u0921\u093c\u0947 \u0939\u093e\u0925\u094b\u0902 \u0932\u093f\u092f\u093e \u0939\u0948\u0964 \u0938\u093e\u0925 \u0939\u0940, \u0905\u092a\u0942\u0930\u094d\u0935 \u0928\u0947 \u0907\u0938 \u092b\u093f\u0932\u094d\u092e \u0915\u094b \u092b\u094d\u0932\u0949\u092a \u092b\u093f\u0932\u094d\u092e \u0915\u093e \u0932\u0947\u092c\u0932 \u092d\u0940 \u0926\u0947 \u0926\u093f\u092f\u093e \u0939\u0948\u0964\u0909\u0928\u094d\u0939\u094b\u0902\u0928\u0947 \u0932\u093f\u0916\u093e, '\u0939\u093f\u091f \u092b\u093f\u0932\u094d\u092e \u0915\u093e \u0906\u092a\u0915\u093e \u092b\u0930\u094d\u091c\u0940 \u092b\u0948\u0938\u0932\u093e \u0926\u0941\u0937\u094d\u091f \u0936\u0915\u094d\u0924\u093f \u0915\u094b \u0914\u0930 \u091c\u094d\u092f\u093e\u0926\u093e \u0936\u0915\u094d\u0924\u093f\u0936\u093e\u0932\u0940 \u092c\u0928\u093e \u0930\u0939\u093e \u0939\u0948\u0964 \u092b\u093f\u0932\u094d\u092e 130 \u0915\u0930\u094b\u0921\u093c \u0930\u0941\u092a\u092f\u094b\u0902 \u0938\u0947 \u092c\u0928\u0940 \u0939\u0948 \u0914\u0930 \u0915\u092e \u0938\u0947 \u0915\u092e \u0909\u0938\u0947 \u0909\u0924\u0928\u093e \u0915\u092e\u093e\u0928\u093e \u091a\u093e\u0939\u093f\u090f\u0964 \u0932\u0947\u0915\u093f\u0928 \u0905\u092d\u0940 \u0924\u0915 \u092b\u093f\u0932\u094d\u092e \u0928\u0947 60 \u0915\u0930\u094b\u0921\u093c \u0915\u0940 \u0915\u092e\u093e\u0908 \u0939\u0948 \u0914\u0930 \u092f\u0939 \u0915\u092e \u0939\u094b\u0924\u0940 \u091c\u093e \u0930\u0939\u0940 \u0939\u0948\u0964 \u0906\u092e\u0924\u094c\u0930 \u092a\u0930 \u0910\u0938\u0940 \u092b\u093f\u0932\u094d\u092e\u094b\u0902 \u0915\u094b \u092b\u094d\u0932\u0949\u092a \u0939\u0940 \u0915\u0939\u093e \u091c\u093e\u0924\u093e \u0939\u0948\u0964'\u092c\u0924\u093e \u0926\u0947\u0902 \u0915\u093f 5 \u0926\u093f\u0928 \u0915\u0947 \u092c\u093e\u0926 \u0915\u0902\u0917\u0928\u093e \u0915\u0940 '\u092e\u0923\u093f\u0915\u0930\u094d\u0923\u093f\u0915\u093e' \u0928\u0947 \u0918\u0930\u0947\u0932\u0942 \u092c\u0949\u0915\u094d\u0938 \u0911\u092b\u093f\u0938 \u092a\u0930 50 \u0915\u0930\u094b\u0921\u093c \u0938\u0947 \u091c\u094d\u092f\u093e\u0926\u093e \u0915\u0940 \u0915\u092e\u093e\u0908 \u0915\u0930 \u0932\u0940 \u0939\u0948\u0964 \u092b\u093f\u0932\u094d\u092e \u092e\u0947\u0902 \u0915\u0902\u0917\u0928\u093e \u0915\u0947 \u0905\u0932\u093e\u0935\u093e \u0905\u0924\u0941\u0932 \u0915\u0941\u0932\u0915\u0930\u094d\u0923\u0940, \u091c\u093f\u0936\u0942 \u0938\u0947\u0928\u0917\u0941\u092a\u094d\u0924\u093e, \u0905\u0902\u0915\u093f\u0924\u093e \u0932\u094b\u0916\u0902\u0921\u0947 \u0914\u0930 \u092e\u094b\u0939\u092e\u094d\u092e\u0926 \u091c\u0940\u0936\u093e\u0928 \u0905\u092f\u0942\u092c \u092d\u0940 \u092a\u094d\u0930\u092e\u0941\u0916 \u092d\u0942\u092e\u093f\u0915\u093e\u0913\u0902 \u092e\u0947\u0902 \u0939\u0948\u0902\u0964"}