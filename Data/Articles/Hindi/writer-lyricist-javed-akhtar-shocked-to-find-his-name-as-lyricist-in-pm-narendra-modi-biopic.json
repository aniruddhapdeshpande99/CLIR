256672000\nMujhe hairat hai apana naam pm narendra Modi film me dekh karr , Maine aisi kisi film me koi gaana nahi likha hai .. \u2014 Sameer (@SameerAnjaan) 1553271345000\n\u092a\u094d\u0930\u0927\u093e\u0928\u092e\u0902\u0924\u094d\u0930\u0940 \u0928\u0930\u0947\u0902\u0926\u094d\u0930 \u092e\u094b\u0926\u0940 \u0915\u0940 \u0932\u093e\u0907\u092b \u092a\u0930 \u092c\u0928 \u0930\u0939\u0940 \u092b\u093f\u0932\u094d\u092e ' \u092a\u0940\u090f\u092e \u0928\u0930\u0947\u0902\u0926\u094d\u0930 \u092e\u094b\u0926\u0940 ' \u0915\u093e \u0939\u093e\u0932 \u0939\u0940 \u092e\u0947\u0902 \u091f\u094d\u0930\u0947\u0932\u0930 \u0930\u093f\u0932\u0940\u091c\u093c \u0915\u093f\u092f\u093e \u0917\u092f\u093e\u0964 \u092b\u093f\u0932\u094d\u092e \u092e\u0947\u0902 \u092a\u0940\u090f\u092e \u0915\u0940 \u092e\u0941\u0916\u094d\u092f \u092d\u0942\u092e\u093f\u0915\u093e \u092e\u0947\u0902 \u092c\u0949\u0932\u093f\u0935\u0941\u0921 \u0910\u0915\u094d\u091f\u0930 \u0935\u093f\u0935\u0947\u0915 \u0913\u092c\u0947\u0930\u0949\u092f \u0939\u0948\u0902\u0964 \u0930\u093f\u0932\u0940\u091c\u093c \u0939\u094b\u0924\u0947 \u0939\u0940 \u091f\u094d\u0930\u0947\u0932\u0930 \u0907\u0902\u091f\u0930\u0928\u0947\u091f \u092a\u0930 \u091b\u093e \u0917\u092f\u093e\u0964 \u0939\u093e\u0932\u093e\u0902\u0915\u093f \u0915\u0941\u091b \u0932\u094b\u0917\u094b\u0902 \u0915\u094b \u092f\u0939 \u0916\u093e\u0938 \u092a\u0938\u0902\u0926 \u0928\u0939\u0940\u0902 \u0906\u092f\u093e\u0964 \u0905\u092c \u0907\u0938 \u092b\u093f\u0932\u094d\u092e \u0915\u093e \u090f\u0915 \u092a\u094b\u0938\u094d\u091f\u0930 \u0930\u093f\u0932\u0940\u091c\u093c \u0915\u093f\u092f\u093e \u0917\u092f\u093e \u0939\u0948, \u091c\u093f\u0938\u092e\u0947\u0902 \u092b\u093f\u0932\u094d\u092e \u0915\u0947 \u0928\u093f\u0930\u094d\u0926\u0947\u0936\u0915 \u0938\u0947 \u0932\u0947\u0915\u0930 \u0938\u0902\u0917\u0940\u0924\u0915\u093e\u0930 \u0914\u0930 \u0915\u093e\u0938\u094d\u091f \u0924\u0915 \u0915\u0947 \u092c\u093e\u0930\u0947 \u092e\u0947\u0902 \u092a\u0942\u0930\u0940 \u091c\u093e\u0928\u0915\u093e\u0930\u0940 \u0926\u0940 \u0917\u0908 \u0939\u0948\u0964\u092b\u093f\u0932\u094d\u092e \u0915\u0947 \u0917\u0940\u0924\u0915\u093e\u0930 \u0915\u0947 \u0930\u0942\u092a \u092e\u0947\u0902 \u092a\u094d\u0930\u0938\u0942\u0928 \u091c\u094b\u0936\u0940 \u0914\u0930 \u0938\u092e\u0940\u0930 \u0915\u0947 \u0905\u0932\u093e\u0935\u093e \u091c\u093e\u0935\u0947\u0926 \u0905\u0916\u094d\u0924\u0930 \u0915\u093e \u092d\u0940 \u0928\u093e\u092e \u0939\u0948\u0964 \u092a\u094b\u0938\u094d\u091f\u0930 \u092e\u0947\u0902 \u0938\u092c\u0938\u0947 \u0928\u0940\u091a\u0947 \u0935\u093e\u0932\u0940 \u0932\u093e\u0907\u0928 \u092e\u0947\u0902 \u092a\u0939\u0932\u093e \u0928\u093e\u092e \u091c\u093e\u0935\u0947\u0926 \u0905\u0916\u094d\u0924\u0930 \u0915\u093e \u0939\u0940 \u0932\u093f\u0916\u093e \u0939\u0948\u0964 \u0932\u0947\u0915\u093f\u0928 \u0935\u0939 \u0905\u092a\u0928\u093e \u0928\u093e\u092e \u0926\u0947\u0916 \u0939\u0948\u0930\u093e\u0928 \u0939\u0948\u0902\u0964 \u091c\u093e\u0935\u0947\u0926 \u0915\u093e \u0915\u0939\u0928\u093e \u0939\u0948 \u0915\u093f \u0909\u0928\u094d\u0939\u094b\u0902\u0928\u0947 \u0907\u0938 \u092b\u093f\u0932\u094d\u092e \u0915\u0947 \u0932\u093f\u090f \u090f\u0915 \u092d\u0940 \u0917\u093e\u0928\u093e \u0928\u0939\u0940\u0902 \u0932\u093f\u0916\u093e \u0939\u0948 \u0914\u0930 \u092c\u093e\u0935\u091c\u0942\u0926 \u0909\u0938\u0915\u0947 \u0915\u094d\u0930\u0947\u0921\u093f\u091f\u094d\u0938 \u092e\u0947\u0902 \u0909\u0928\u0915\u093e \u0928\u093e\u092e \u0936\u093e\u092e\u093f\u0932 \u0915\u093f\u092f\u093e \u0917\u092f\u093e \u0939\u0948\u0964\u091c\u093e\u0935\u0947\u0926 \u0905\u0916\u094d\u0924\u0930 \u0928\u0947 \u0907\u0938\u0915\u0940 \u091c\u093e\u0928\u0915\u093e\u0930\u0940 '\u092a\u0940\u090f\u092e \u0928\u0930\u0947\u0902\u0926\u094d\u0930 \u092e\u094b\u0926\u0940' \u0915\u0947 \u092a\u094b\u0938\u094d\u091f\u0930 \u0915\u094b \u0936\u0947\u092f\u0930 \u0915\u0930\u0924\u0947 \u0939\u0941\u090f \u0926\u0940\u0964 \u0909\u0928\u094d\u0939\u094b\u0902\u0928\u0947 \u091f\u094d\u0935\u0940\u091f \u0915\u093f\u092f\u093e, '\u092e\u0948\u0902 \u0907\u0938 \u092b\u093f\u0932\u094d\u092e \u0915\u0947 \u092a\u094b\u0938\u094d\u091f\u0930 \u092a\u0930 \u0905\u092a\u0928\u093e \u0928\u093e\u092e \u0926\u0947\u0916\u0915\u0930 \u0938\u094d\u0924\u092c\u094d\u0927 \u0939\u0942\u0902\u0964 \u092e\u0948\u0902\u0928\u0947 \u0907\u0938 \u092b\u093f\u0932\u094d\u092e \u0915\u0947 \u0932\u093f\u090f \u0915\u094b\u0908 \u0917\u093e\u0928\u093e \u0928\u0939\u0940\u0902 \u0932\u093f\u0916\u093e \u0939\u0948\u0964'\u0935\u0939\u0940\u0902, \u0917\u0940\u0924\u0915\u093e\u0930 \u0938\u092e\u0940\u0930 \u0928\u0947 \u092d\u0940 \u092b\u093f\u0932\u094d\u092e \u0915\u0947 \u0915\u094d\u0930\u0947\u0921\u093f\u091f \u092a\u094b\u0938\u094d\u091f\u0930 \u092e\u0947\u0902 \u0905\u092a\u0928\u093e \u0928\u093e\u092e \u0939\u094b\u0928\u0947 \u092a\u0930 \u0939\u0948\u0930\u093e\u0928\u0940 \u091c\u0924\u093e\u0908 \u0939\u0948\u0964 \u0909\u0928\u094d\u0939\u094b\u0902\u0928\u0947 \u091f\u094d\u0935\u0940\u091f \u0915\u0930\u0924\u0947 \u0939\u0941\u090f \u0932\u093f\u0916\u093e, '\u092e\u0941\u091d\u0947 \u0939\u0948\u0930\u0924 \u0939\u0948 '\u092a\u0940\u090f\u092e \u0928\u0930\u0947\u0902\u0926\u094d\u0930 \u092e\u094b\u0926\u0940' \u092b\u093f\u0932\u094d\u092e \u092e\u0947\u0902 \u0905\u092a\u0928\u093e \u0928\u093e\u092e \u0926\u0947\u0916\u0915\u0930\u0964 \u092e\u0948\u0902\u0928\u0947 \u0910\u0938\u0940 \u0915\u093f\u0938\u0940 \u092b\u093f\u0932\u094d\u092e \u092e\u0947\u0902 \u0915\u094b\u0908 \u0917\u093e\u0928\u093e \u0928\u0939\u0940\u0902 \u0932\u093f\u0916\u093e\u0964'\u0935\u0948\u0938\u0947 \u092c\u0924\u093e \u0926\u0947\u0902 \u0915\u093f \u092f\u0939 \u092a\u094b\u0938\u094d\u091f\u0930 \u0913\u0930\u093f\u091c\u0928\u0932 \u0939\u0948 \u092f\u093e \u0928\u0939\u0940\u0902, \u0907\u0938\u0915\u0940 \u0905\u092d\u0940 \u092a\u0941\u0937\u094d\u091f\u093f \u0928\u0939\u0940\u0902 \u0915\u0940 \u0917\u0908 \u0939\u0948\u0964 '\u092a\u0940\u090f\u092e \u0928\u0930\u0947\u0902\u0926\u094d\u0930 \u092e\u094b\u0926\u0940' \u0915\u093e \u0928\u093f\u0930\u094d\u0926\u0947\u0936\u0928 \u0913\u092e\u0902\u0917 \u0915\u0941\u092e\u093e\u0930 \u0928\u0947 \u0915\u093f\u092f\u093e \u0939\u0948 \u0914\u0930 \u0907\u0938\u092e\u0947\u0902 \u092e\u094b\u0926\u0940 \u0915\u0947 \u092c\u091a\u092a\u0928 \u0938\u0947 \u0932\u0947\u0915\u0930 \u092a\u094d\u0930\u0927\u093e\u0928\u092e\u0902\u0924\u094d\u0930\u0940 \u092c\u0928\u0928\u0947 \u0924\u0915 \u0915\u0947 \u0938\u0902\u0918\u0930\u094d\u0937 \u0915\u094b \u0926\u093f\u0916\u093e\u092f\u093e \u091c\u093e\u090f\u0917\u093e\u0964 \u092b\u093f\u0932\u094d\u092e \u092e\u0947\u0902 \u0935\u093f\u0935\u0947\u0915 \u0913\u092c\u0947\u0930\u0949\u092f \u0915\u0947 \u0905\u0932\u093e\u0935\u093e \u092c\u092e\u0928 \u0907\u0930\u093e\u0928\u0940, \u092c\u0930\u0916\u093e \u092c\u093f\u0937\u094d\u091f, \u092e\u0928\u094b\u091c \u091c\u094b\u0936\u0940, \u092a\u094d\u0930\u0936\u093e\u0902\u0924 \u0928\u093e\u0930\u093e\u092f\u0923, \u0930\u093e\u091c\u0947\u0902\u0926\u094d\u0930 \u0917\u0941\u092a\u094d\u0924\u093e, \u091c\u0930\u0940\u0928\u093e \u0935\u0939\u093e\u092c \u0914\u0930 \u0905\u0902\u091c\u0928 \u0936\u094d\u0930\u0940\u0935\u093e\u0938\u094d\u0924\u0935 \u092d\u0940 \u092e\u0941\u0916\u094d\u092f \u092d\u0942\u092e\u093f\u0915\u093e\u0913\u0902 \u092e\u0947\u0902 \u0926\u093f\u0916\u093e\u0908 \u0926\u0947\u0902\u0917\u0947\u0964 \u092f\u0939 \u092b\u093f\u0932\u094d\u092e 5 \u0905\u092a\u094d\u0930\u0948\u0932 \u0915\u094b \u0930\u093f\u0932\u0940\u091c \u0939\u094b\u0917\u0940\u0964"}