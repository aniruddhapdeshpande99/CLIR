02 \u0930\u093f\u0932\u0940\u091c\u093c \u0939\u0941\u0908 \u0925\u0940\u0964\n\u0926\u093f\u0936\u093e \u0928\u0947 \u091f\u093e\u0907\u0917\u0930 \u0936\u094d\u0930\u0949\u092b \u0915\u0947 \u0938\u093e\u0925 \u090f\u0915 \u092e\u094d\u092f\u0942\u091c\u093f\u0915 \u0935\u093f\u0921\u093f\u092f\u094b \u092d\u0940 \u0915\u093f\u092f\u093e \u0925\u093e \u091c\u093f\u0938\u0947 \u092f\u0942\u091f\u094d\u092f\u0942\u092c \u092a\u0930 3 \u0915\u0930\u094b\u0921\u093c 20 \u0932\u093e\u0916 \u0938\u0947 \u091c\u094d\u092f\u093e\u0926\u093e \u0935\u094d\u092f\u0942\u091c\u093c \u092e\u093f\u0932\u0947\u0964\n\u0926\u093f\u0936\u093e \u0928\u0947 \u0907\u0902\u091f\u0930\u0928\u0948\u0936\u0928\u0932 \u0938\u0941\u092a\u0930\u0938\u094d\u091f\u093e\u0930 \u091c\u0948\u0915\u0940 \u091a\u0948\u0928 \u0915\u0947 \u0938\u093e\u0925 \u092b\u093f\u0932\u094d\u092e '\u0915\u0941\u0902\u0917 \u092b\u0942 \u092f\u094b\u0917\u093e' \u092e\u0947\u0902 \u092d\u0940 \u0915\u093e\u092e \u0915\u093f\u092f\u093e \u0925\u093e\u0964 \u092f\u0939 \u092b\u093f\u0932\u094d\u092e \u0938\u093e\u0932 2017 \u092e\u0947\u0902 \u0930\u093f\u0932\u0940\u091c\u093c \u0939\u0941\u0908 \u0925\u0940 \u0914\u0930 \u0907\u0938\u0928\u0947 1200 \u0915\u0930\u094b\u0921\u093c \u0915\u093e \u092c\u093f\u091c\u0928\u0938 \u0915\u093f\u092f\u093e \u0925\u093e\u0964\n\u0926\u093f\u0936\u093e \u0938\u094b\u0936\u0932 \u092e\u0940\u0921\u093f\u092f\u093e \u092a\u0930 \u0915\u093e\u092b\u0940 \u0910\u0915\u094d\u091f\u093f\u0935 \u0930\u0939\u0924\u0940 \u0939\u0948\u0902\u0964 \u092b\u094b\u091f\u094b \u0936\u0947\u092f\u0930\u093f\u0902\u0917 \u0938\u093e\u0907\u091f \u0907\u0902\u0938\u094d\u091f\u093e\u0917\u094d\u0930\u093e\u092e \u092a\u0930 \u0909\u0928\u0915\u0947 70 \u0932\u093e\u0916 \u0938\u0947 \u091c\u094d\u092f\u093e\u0926\u093e \u092b\u0949\u0932\u094b\u0905\u0930 \u0939\u0948\u0902\u0964\n\u0910\u0938\u093e \u0915\u0939\u093e \u091c\u093e\u0924\u093e \u0939\u0948 \u0915\u093f \u0926\u093f\u0936\u093e \u0910\u0915\u094d\u091f\u0930 \u091f\u093e\u0907\u0917\u0930 \u0936\u094d\u0930\u0949\u092b \u0915\u0947 \u0938\u093e\u0925 \u0930\u093f\u0932\u0947\u0936\u0928\u0936\u093f\u092a \u092e\u0947\u0902 \u0939\u0948\u0902\u0964 \u0915\u0908 \u092e\u094c\u0915\u094b\u0902 \u092a\u0930 \u0907\u0938 \u0915\u092a\u0932 \u0915\u094b \u0938\u093e\u0925 \u0926\u0947\u0916\u093e \u091c\u093e\u0924\u093e \u0930\u0939\u093e \u0939\u0948\u0964 \u0926\u093f\u0936\u093e, \u091f\u093e\u0907\u0917\u0930 \u0915\u0947 \u0938\u093e\u0925 \u092b\u093f\u0932\u094d\u092e '\u092c\u093e\u0917\u0940 2' \u092e\u0947\u0902 \u0928\u091c\u0930 \u0906\u090f\u0902\u0917\u0940\u0964\n\u092e\u0949\u0921\u0932\u093f\u0902\u0917 \u092e\u0947\u0902 \u0906\u0928\u0947 \u0938\u0947 \u092a\u0939\u0932\u0947 \u0926\u093f\u0936\u093e \u0910\u092e\u093f\u091f\u0940 \u092f\u0942\u0928\u093f\u0935\u0930\u094d\u0938\u093f\u091f\u0940 \u0938\u0947 \u092c\u0940.\u091f\u0947\u0915 \u0915\u0930 \u0930\u0939\u0940 \u0925\u0940\u0902\u0964 \u092c\u093e\u0926 \u092e\u0947\u0902 \u092e\u0949\u0921\u0932\u093f\u0902\u0917 \u0915\u0947 \u0911\u092b\u0930 \u092e\u093f\u0932\u0928\u0947 \u0915\u0947 \u092c\u093e\u0926 \u0907\u0902\u091c\u093f\u0928\u093f\u092f\u0930\u093f\u0902\u0917 \u091b\u094b\u0921\u093c \u0926\u0940 \u0914\u0930 \u0935\u0939 \u092e\u0941\u0902\u092c\u0908 \u0906 \u0917\u0908\u0902\u0964\n\u0926\u093f\u0936\u093e \u092a\u093e\u091f\u0928\u0940 \u0915\u094b \u092a\u0922\u093c\u0928\u0947 \u0915\u093e \u0915\u093e\u092b\u0940 \u0936\u094c\u0915 \u0939\u0948\u0964 \u0907\u0938\u0915\u0947 \u0905\u0932\u093e\u0935\u093e \u091c\u093f\u092e\u0928\u093e\u0938\u094d\u091f\u093f\u0915 \u092d\u0940 \u0909\u0928\u0915\u0940 \u092b\u0947\u0935\u0930\u093f\u091f \u0939\u0949\u092c\u0940 \u092e\u0947\u0902 \u0936\u0941\u092e\u093e\u0930 \u0939\u0948\u0964\n\u092c\u0949\u0932\u093f\u0935\u0941\u0921 \u092e\u0947\u0902 \u0930\u0923\u092c\u0940\u0930 \u0915\u092a\u0942\u0930, \u092e\u093e\u0927\u0941\u0930\u0940 \u0926\u0940\u0915\u094d\u0937\u093f\u0924, \u0915\u091f\u0930\u0940\u0928\u093e \u0915\u0948\u092b, \u092a\u094d\u0930\u093f\u092f\u0902\u0915\u093e \u091a\u094b\u092a\u0921\u093c\u093e \u0914\u0930 \u0905\u0915\u094d\u0937\u092f \u0915\u0941\u092e\u093e\u0930 \u092b\u0947\u0935\u0930\u093f\u091f \u0910\u0915\u094d\u091f\u0930\u094d\u0938 \u0939\u0948\u0902\u0964 \u0907\u0938\u0915\u0947 \u0905\u0932\u093e\u0935\u093e \u0926\u093f\u0936\u093e \u0915\u094b \u091c\u093f\u092e \u0915\u0948\u0930\u0940 \u0914\u0930 \u0932\u093f\u092f\u0928\u093e\u0930\u094d\u0926\u094b \u0926\u093f\u0915\u0948\u092a\u094d\u0930\u093f\u092f\u094b \u092d\u0940 \u0915\u093e\u092b\u0940 \u092a\u0938\u0902\u0926 \u0939\u0948\u0902\u0964\nX\n\u092a\u0939\u0932\u0947 \u0910\u0938\u0940 \u0916\u092c\u0930\u0947\u0902 \u0925\u0940\u0902 \u0915\u093f \u092e\u094b\u0939\u093f\u0924 \u0938\u0942\u0930\u0940 \u0915\u0940 \u0905\u0917\u0932\u0940 \u092b\u093f\u0932\u094d\u200d\u092e \u092e\u0947\u0902 \u0915\u0943\u0924\u093f \u0938\u0947\u0928\u0928 \u0915\u094b \u0915\u093e\u0938\u094d\u200d\u091f \u091c\u093e \u0930\u0939\u093e \u0939\u0948\u0964 \u0939\u093e\u0932\u093e\u0902\u0915\u093f, \u0905\u092c \u0907\u0938 \u092a\u094d\u0930\u0949\u091c\u0947\u0915\u094d\u200d\u091f \u0938\u0947 \u0926\u093f\u0936\u093e \u092a\u093e\u091f\u0928\u0940 \u0915\u093e \u0928\u093e\u092e \u091c\u0941\u0921\u093c \u0917\u092f\u093e \u0939\u0948\u0964\u0926\u0930\u0905\u0938\u0932, \u0907\u0938 \u0930\u094b\u092e\u093e\u0902\u091f\u093f\u0915 \u0910\u0915\u094d\u200d\u0936\u0928-\u0925\u094d\u0930\u093f\u0932\u0930 \u092e\u0947\u0902 \u0905\u092c \u0906\u0926\u093f\u0924\u094d\u200d\u092f \u0930\u0949\u092f \u0915\u092a\u0942\u0930 \u0915\u0947 \u0911\u092a\u094b\u091c\u093f\u091f \u0926\u093f\u0936\u093e \u0915\u094b \u0915\u093e\u0938\u094d\u200d\u091f \u0915\u093f\u090f \u091c\u093e\u0928\u0947 \u0915\u0940 \u092c\u093e\u0924 \u091a\u0932 \u0930\u0939\u0940 \u0939\u0948\u0964 \u092f\u0939 \u092a\u0939\u0932\u093e \u092e\u094c\u0915\u093e \u0939\u094b\u0917\u093e \u091c\u092c \u0926\u094b\u0928\u094b\u0902 \u0915\u0940 \u091c\u094b\u0921\u093c\u0940 \u0915\u093f\u0938\u0940 \u092b\u093f\u0932\u094d\u200d\u092e \u092e\u0947\u0902 \u0928\u091c\u0930 \u0906\u090f\u0917\u0940\u0964\u0907\u0938 \u092a\u094d\u0930\u0949\u091c\u0947\u0915\u094d\u200d\u091f \u0938\u0947 \u091c\u0941\u0921\u093c\u0947 \u090f\u0915 \u0938\u0942\u0924\u094d\u0930 \u0928\u0947 \u092c\u0924\u093e\u092f\u093e, '\u092e\u0941\u0902\u092c\u0908 \u092e\u0947\u0902 \u090f\u0915 \u092c\u0921\u093c\u093e \u0938\u0947\u091f \u0932\u0917\u093e\u092f\u093e \u091c\u093e \u0930\u0939\u093e \u0939\u0948\u0964 \u0906\u0928\u0947 \u0935\u093e\u0932\u0947 \u0915\u0941\u091b \u092e\u0939\u0940\u0928\u094b\u0902 \u092e\u0947\u0902 \u092b\u093f\u0932\u094d\u200d\u092e \u092b\u094d\u0932\u094b\u0930 \u092a\u0930 \u091c\u093e\u090f\u0917\u0940\u0964' \u0907\u0938\u0938\u0947 \u092a\u0939\u0932\u0947 \u092e\u093e\u0930\u094d\u091a 2018 \u092e\u0947\u0902 \u0939\u092e\u093e\u0930\u0947 \u0938\u0939\u092f\u094b\u0917\u0940 \u092e\u093f\u0930\u0930 \u0938\u0947 \u092c\u093e\u0924\u091a\u0940\u0924 \u0915\u0947 \u0926\u094c\u0930\u093e\u0928 \u0910\u0915\u094d\u200d\u091f\u094d\u0930\u0947\u0938 \u0928\u0947 \u092e\u094b\u0939\u093f\u0924 \u0938\u0942\u0930\u0940 \u0915\u0947 \u0938\u093e\u0925 \u0915\u093e\u092e \u0915\u0930\u0928\u0947 \u0915\u0940 \u0916\u092c\u0930 \u0915\u094b \u0916\u093e\u0930\u093f\u091c \u0915\u0930 \u0926\u093f\u092f\u093e \u0925\u093e\u0964\u0909\u0927\u0930, \u091c\u092c \u092e\u094b\u0939\u093f\u0924 \u0928\u0947 \u0914\u0930 \u0910\u0915\u094d\u200d\u091f\u0930\u094d\u0938 \u0915\u0947 \u0915\u093e\u0938\u094d\u200d\u091f \u0915\u0947 \u0938\u093e\u0925 \u091c\u0941\u0921\u093c\u0928\u0947 \u0915\u0940 \u092c\u093e\u0924 \u0915\u0939\u0940 \u0925\u0940 \u0924\u094b \u0906\u0926\u093f\u0924\u094d\u200d\u092f \u0914\u0930 \u0909\u0928\u0915\u0947 \u092c\u0940\u091a \u0905\u0928\u092c\u0928 \u0915\u0940 \u092d\u0940 \u091a\u0930\u094d\u091a\u093e \u0939\u094b\u0928\u0947 \u0932\u0917\u0940 \u0925\u0940\u0964 \u0939\u093e\u0932\u093e\u0902\u0915\u093f, \u092c\u093e\u0926 \u092e\u0947\u0902 \u0921\u093e\u092f\u0930\u0947\u0915\u094d\u200d\u091f\u0930 \u0928\u0947 \u0907\u0928 \u092c\u093e\u0924\u094b\u0902 \u0938\u0947 \u0907\u0928\u0915\u093e\u0930 \u0915\u0930 \u0926\u093f\u092f\u093e \u0925\u093e\u0964\u0907\u0938 \u092b\u093f\u0932\u094d\u200d\u092e \u0938\u0947 \u0906\u0926\u093f\u0924\u094d\u200d\u092f \u0914\u0930 \u092e\u094b\u0939\u093f\u0924 \u0915\u0940 \u091c\u094b\u0921\u093c\u0940 \u092b\u093f\u0930 \u0938\u0947 \u0926\u0930\u094d\u0936\u0915\u094b\u0902 \u0915\u0947 \u092c\u0940\u091a \u0939\u094b\u0917\u0940\u0964 \u0907\u0938\u0938\u0947 \u092a\u0939\u0932\u0947 \u0926\u094b\u0928\u094b\u0902 \u0928\u0947 2013 \u092e\u0947\u0902 \u0906\u0908 \u0932\u0935 \u0938\u094d\u200d\u091f\u094b\u0930\u0940 '\u0906\u0936\u093f\u0915\u0940 2' \u092e\u0947\u0902 \u0915\u093e\u092e \u0915\u093f\u092f\u093e \u0925\u093e\u0964 \u0921\u093e\u092f\u0930\u0947\u0915\u094d\u200d\u091f\u0930 \u0938\u0942\u0930\u0940 \u0915\u0947 \u0938\u093e\u0925 \u0926\u093f\u0936\u093e \u0915\u0940 \u092f\u0939 \u092a\u0939\u0932\u0940 \u092b\u093f\u0932\u094d\u200d\u092e \u0939\u094b\u0917\u0940\u0964"}